{{appClass 'app-set'}}
{{#menu 'submenu'}}
<li>
	<a href="#/new" class="btn">New set</a>
</li>
{{/menu}}

<div id="new">
	<form action="/service/add">
		<fieldset>
			<label for="NameTextBox">Name</label>
			<input type="text" name="name" id="NameTextBox" placeholder="Frontal cat faces..." />
			<label for="PathTextBox">Path</label>
			<input type="text" name="path" id="PathTextBox" placeholder="/path/to/files..." />
		</fieldset>
	</form>
</div>
